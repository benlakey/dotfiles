{
	"name": "status-bar-tasks",
	"displayName": "Status Bar Tasks",
	"description": "Load your VS Code Tasks into executable Status Bar commands.",
	"version": "0.2.1",
	"publisher": "GuardRex",
	"repository": {
		"type": "git",
		"url": "https://github.com/GuardRex/vscode-status-bar-tasks"
	},
	"icon": "guardrex-logo.png",
	"galleryBanner": {
		"color": "#d9f6ff",
		"theme": "light"
	},
	"bugs": {
		"url": "https://github.com/GuardRex/vscode-status-bar-tasks/issues"
	},
	"categories": [
		"Other"
	],
	"engines": {
		"vscode": "^1.9.0"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [],
		"jsonValidation": [
			{
				"fileMatch": "/.vscode/tasks.json",
				"url": "./custom-tasks-schema.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"strip-json-comments": "^2.0.1",
		"typescript": "^2.0.3",
		"vscode": "^1.0.3",
		"@types/node": "^7.0.5",
		"@types/mocha": "^2.2.39"
	},
	"__metadata": {
		"id": "9d34632f-9ee4-4b6d-9d52-11cb9a58257b",
		"publisherId": "ce172fda-e75f-46fd-89e9-0bd3ad9eedf7",
		"publisherDisplayName": "GuardRex"
	}
}